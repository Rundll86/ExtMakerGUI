{
    "compilerOptions": {
        "skipLibCheck": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "generated"
    },
    "include": [
        "./src/**/*.ts"
    ]
}